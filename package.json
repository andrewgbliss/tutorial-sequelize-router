{
  "name": "tutorial-sequelize-router",
  "version": "1.0.0",
  "engines": {
    "node": ">=14.5.0",
    "npm": ">=6.14.5"
  },
  "scripts": {
    "start": "node ./build/index.js",
    "build": "node --max-old-space-size=8192 node_modules/typescript/bin/tsc",
    "dev": "ts-node-dev --files --watch -- index.ts",
    "test": "NODE_ENV=test ts-mocha ./**/*.spec.ts",
    "sequelize": "cd ./build/db && sequelize",
    "clean": "rm -Rf node_modules package-lock.json && npm i",
    "migrate": "npm run sequelize db:migrate",
    "seed": "npm run sequelize db:seed:all",
    "heroku-postbuild": "npm run build",
    "release": "npm run build && npm run migrate && npm run seed"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.3.0",
    "sequelize": "^6.3.0",
    "sequelize-cli": "6.2.0",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "@testing-library/user-event": "^12.0.11",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.158",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.20",
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "ts-mocha": "^7.0.0",
    "ts-node-dev": "1.0.0-pre.51"
  }
}